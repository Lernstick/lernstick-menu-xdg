#: usr/bin/lernstick_backup:66
#, sh-format
msgid ""
"The file system on the backup device (<tt>${DEVICE}</tt>) seems to be "
"corrupt.<br>An interactive file system repair will be started..."
msgstr ""
"Das Dateisystem auf dem Datensicherungsgerät (<tt>${DEVICE}</tt>) scheint "
"defekt zu sein.<br>Eine interaktive Dateisystemreparatur wird gestartet..."

#: usr/bin/lernstick_backup:69
#, sh-format
msgid "File system check finished.\\nYou can close this window now."
msgstr "Die Dateisystemreparatur ist beendet.\\nSie können dieses Fenster nun schließen."

#: usr/bin/lernstick_backup:70
#, sh-format
msgid "File System Repair"
msgstr "Dateisystemreparatur"

#: usr/bin/lernstick_backup:94
#, sh-format
msgid "Backup to<br><tt>${DESTINATION}</tt><br>failed!"
msgstr "Datensicherung nach<br><tt>${DESTINATION}</tt><br>fehlgeschlagen!"

#: usr/bin/lernstick_backup:110
#, sh-format
msgid "There is no free space left on device <tt>${DEVICE}</tt>!"
msgstr "Auf dem Gerät <tt>${DEVICE}</tt> ist kein freier Speicherplatz mehr!"

#: usr/bin/lernstick_backup:188
#, sh-format
msgid "Backup partition<br><tt>${BackupPartition}</tt><br>not found!"
msgstr ""
"Datensicherungspartition<br><tt>${BackupPartition}</tt><br>nicht gefunden!"
